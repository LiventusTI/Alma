            document.addEventListener('DOMContentLoaded', function () {
                var sidebarTreeview = document.getElementById("sidebar-treeview").ej2_instances[0];
                var treeviewObj = document.getElementById("main-treeview").ej2_instances[0];

                // Toggle the Sidebar
                document.getElementById('header-section').onclick = function () {
                    if (sidebarTreeview.isOpen) {
                        sidebarTreeview.hide();
                        treeviewObj.collapseAll();
                    }
                    else {
                        sidebarTreeview.show();
                        treeviewObj.expandAll();
                    }
                }
            });

            function onCreate() {
                // Hide the sidebar element on creation using isOpen property 
                var sidebarTreeview = document.getElementById("sidebar-treeview").ej2_instances[0];
                var treeviewObj = document.getElementById("main-treeview").ej2_instances[0];
                sidebarTreeview.show();
                treeviewObj.expandAll();
            }

          

            function onClose() {
                var treeviewObj = document.getElementById("main-treeview").ej2_instances[0];
                treeviewObj.collapseAll();
            }

            function node() {
                // Perform custom operation
