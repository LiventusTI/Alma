    <style>
        #container {
            position: relative;
            padding: 40px;
            border: grey 3px solid;
        }

        #dvLoading {
            background: url(../images/loader.gif) no-repeat center center;
            height: 50px;
            width: 50px;
        }

        #loader {
            background: #ffffff;
            opacity: 0.7;
            bottom: 0;
            height: 100%;
            left: 0;
            position: fixed;
            right: 0;
            top: 0;
            width: 100%;
            z-index: 9999;
            float: left;
            clear: none;
        }

        #loaderInner {
            background: url(../images/loading.gif) center center no-repeat;
            height: 100px;
            left: 45%;
            position: absolute;
            top: 40%;
            width: 200px;
        }

        #logoaplicacion {
            left: 45%;
            position: absolute;
            top: 50%;
        }

        .ui-datepicker table tbody .ui-datepicker-week-col {
            cursor: pointer;
            color: red;
        }
